{
  "tasks": {},
  "imports": {
    "cobweb/jsx-runtime": "./src/jsx.ts"
  },
  "fmt": {
    "useTabs": true,
    "semiColons": true
  },
  "lint": {
    "rules": {
      "tags": ["recommended"],
      "exclude": ["no-explicit-any", "require-await"]
    }
  },
  "compilerOptions": {
    "jsx": "react-jsx",
    "jsxImportSource": "cobweb",
    "lib": ["deno.ns", "esnext", "dom", "dom.iterable"]
  }
}
